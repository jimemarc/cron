    name = Marcos Jimenez
    email = mjmata@hpe.com
[pull]
    rebase = true
[core]
    repositoryformatversion = 0
    filemode = true
    bare = false
    preloadindex = true
    logallrefupdates = true
[remote "origin"]
    url = https://github.com/jimemarc/cron
    fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
    remote = origin
    merge = refs/heads/master
[push]
    default = tracking
[apply]
    whitespace = nowarn
[color]
    ui = true
[color "decorate"]
    tag = blue
[alias]
    st = status
    l = log --name-status --abbrev-commit --decorate
    ls = ls-files
    co = checkout
    br = branch
    ci = commit
    unstage = reset HEAD --
    last = log -1 HEAD
